{
  "name": "kc-mutual-aid-api",
  "version": "1.0.0",
  "description": "Graphql API for KC Mutual Aid Application.",
  "author": "Justin Graber <4graber@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "build-ts": "tsc",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "watch": "yarn build-ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\""
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.5",
    "aws-sdk": "^2.1111.0",
    "body-parser": "^1.20.0",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.3",
    "pg": "^8.7.3",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  }
}
